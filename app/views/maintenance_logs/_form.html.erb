<%= form_for(@maintenance_log) do |f| %>

  <%= f.label :date_performed %>
  <%= f.date_select :date_performed %><br>
  <%= f.label :date_due %>
  <%= f.date_select :date_due %><br>

  <%= f.label :tools %><br>
  <%= f.text_field :tools %><br>

  <%= f.label :cost %><br>
  <%= f.number_field :cost %><br>

  <%= f.label :notes %><br>
  <%= f.text_area :notes %><br>

  <%= f.submit %>
<% end %>